<%@ page contentType="text/html;charset=utf-8"%>
<%@ include file="/WEB-INF/jsp/include.jsp"%>
<script src="${rootUrl }js/datepicker/WdatePicker.js"
	type="text/javascript"></script>
<div style="margin: 10px 20px 0px; padding: 0px">
	<dl class="title">
		<dd>十里河PES中频机架运行情况--更新</dd>
	</dl>
</div>
<table width="100%" border="0">
	<tr>
		<th scope="col">检查内容</th>
		<th scope="col">检查结果（必填）</th>
		<th scope="col">检查方法</th>
	</tr>
	<tr>
		<th scope="col">INR1</th>
		<th scope="col"><form:input path="inr1"
				value="${zpjjstate.inr1 }" /></th>
		<th rowspan="10" scope="col">PES网管中查询</th>
	</tr>
	<tr>
		<th scope="col">INR2</th>
		<th scope="col"><form:input path="inr2"
				value="${zpjjstate.inr2 }" /></th>
	</tr>
	<tr>
		<th scope="col">INR3</th>
		<th scope="col"><form:input path="inr3"
				value="${zpjjstate.inr3 }" /></th>
	</tr>
	<tr>
		<th scope="col">INR4</th>
		<th scope="col"><form:input path="inr4"
				value="${zpjjstate.inr4 }" /></th>
	</tr>
	<tr>
		<th scope="col">INR5</th>
		<th scope="col"><form:input path="inr5"
				value="${zpjjstate.inr5 }" /></th>
	</tr>
	<tr>
		<th scope="col">INR6</th>
		<th scope="col"><form:input path="inr6"
				value="${zpjjstate.inr6 }" /></th>
	</tr>
	<tr>
		<th scope="col">INR7</th>
		<th scope="col"><form:input path="inr7"
				value="${zpjjstate.inr7 }" /></th>
	</tr>
	<tr>
		<th scope="col">备用INR</th>
		<th scope="col"><form:input path="inrb"
				value="${zpjjstate.inrb }" /></th>
	</tr>
	<tr>
		<th scope="col">EBI/No(dB)</th>
		<th scope="col"><form:input path="ebi" value="${zpjjstate.ebi }" /></th>
	</tr>
	<tr>
		<th scope="col">EBT/No(dB)</th>
		<th scope="col"><form:input path="ebt" value="${zpjjstate.ebt }" /></th>
	</tr>
</table>


<div class="boxbtn">
	<c:if test="${!view}">
		<div class="btn">
			<a href="#none" id="submit">保存</a>
		</div>
	</c:if>
	<div class="btn">
		<a href="#none" class="close">取消</a>
	</div>
</div>
