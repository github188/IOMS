public enum OptType {

	ADD("新增"), 
	MODIFY("修改"),
	SUBMIT("提交"), 
	TAKE("接单"), 
	CHANGE("转咨询"),
	TRANSFER("移交"), 
	ASSIGN("分配"),
	SOLVE("值班员解决"), 
	RESOLVE("工程师解决"),
