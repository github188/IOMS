@charset "utf-8";
/* CSS Document */
*{ margin:0px; padding:0px; font-size:12px;}
body { background:#EFF7FE url(../images/mainbg.gif) repeat-x center top; text-align:center;}
.bold{ font-weight:bold;}

#boxtop{ height:27px; border:1px solid #C3CCD3; clear:both;background:url(../images/viewTop.png) repeat-x center bottom;}
#boxtop .place{  padding-bottom:30px ;float:left;height:20px;}
#boxtop .close{  padding:5px 0px 0px 20px;float:right;font-weight:blob;}
#boxtop img{ margin-right:5px;padding:0px 10px 0px 0px;border:0px;vertical-align: middle;}

table{}
table th{ height:25px; text-align:right; font-size:13px; font-weight:blob;}
table td{ color:#1F83B7; height:25px; font-size:13px; color:#000; padding-left:2px;}
table th a{ padding:0px 5px;}

.boxbtn{ height:20px; clear:both; text-align:center;width:100%;}
.sbtn .btn{ height:22px; background:url(../images/btn/btnbg.gif) repeat-x center center; border:1px solid #A9B4BE;color:#104376; cursor:pointer;}
.sbtn{ height:22px;float:left; margin-right:4px;}
.sbtn a:link,.sbtn a:visited,.sbtn a:active{ color:#333; text-decoration:none; display:block; background:url(../images/btn/btnbg.gif) repeat-x center bottom; border:1px solid #95BDD7; width:auto; height:15px; padding:5px 6px 0px 6px;}
.sbtn a:hover{ color:#705D00; text-decoration: none; display:block; background:url(../images/btn/btnbg2.gif) repeat-x center bottom; border:1px solid #705D00;}

.operate{ border:1px solid #95BDD7; border-collapse:collapse;}
.operate th{ border:1px solid #95BDD7; background:#EFF0F2; height:25px; color:#1F83B7; text-align:center; font-size:13px; font-weight:normal;}
.operate td{ border:1px solid #95BDD7; color:#1F83B7; height:25px; font-size:13px; color:#000; padding-left:2px;}