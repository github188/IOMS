package cn.com.atnc.ioms.entity.reports;

import java.util.ArrayList;
import java.util.List;

/**
 * 转报地址基础信息报表实体
 * @author WangLingbin
 * @date 2015年5月28日 下午3:39:18
 * @since 1.0.0
 */
public class TeleGraphBaseInfoEntity {

	private String applyCom;			//申请单位
	private String serialNumber;			//申请编号
	private String linkMan;				//联系人
	private String linkPhone;           //联系电话
	private String applyType;			//申请类型
	private String applyComSign;		//申请单位签字
	private String usingPeriod;			//使用时限
	private String usingArea;			//使用地区
	private String remark;				//备注/其它要求
	private String areaOpition;			//地区网络中心意见
	private String areaSign;			//地区网络中心负责人签字
	private String atncOpition;			//北京网控公司意见
	private String atncSign;			//北京网控公司负责人签字
	private String applyDate;			//申请时间
	private String addressType;			//地址类型
	
	List<TeleGraphApplyEntity> apply = new ArrayList<TeleGraphApplyEntity>();	//申请信息
	List<TeleGraphConfigEntity> config = new ArrayList<TeleGraphConfigEntity>();	//配置信息
	List<TeleGraphCircuitEntity> circuits = new ArrayList<TeleGraphCircuitEntity>();
