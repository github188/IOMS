		super();
	}
	public TeleGraphBaseInfoEntity(String applyCom, String linkMan,
			String linkPhone, String applyType, String applyComSign,
			String usingPeriod, String usingArea, String remark,
			String areaOpition, String areaSign, String atncOpition,
			String atncSign, List<TeleGraphApplyEntity> apply) {
		super();
		this.applyCom = applyCom;
		this.linkMan = linkMan;
		this.linkPhone = linkPhone;
		this.applyType = applyType;
		this.applyComSign = applyComSign;
		this.usingPeriod = usingPeriod;
		this.usingArea = usingArea;
		this.remark = remark;
		this.areaOpition = areaOpition;
		this.areaSign = areaSign;
		this.atncOpition = atncOpition;
		this.atncSign = atncSign;
		this.apply = apply;
	}
	public TeleGraphBaseInfoEntity(String applyCom, String linkMan,
			String linkPhone, String applyType, String applyComSign,
			String usingPeriod, String usingArea, String remark,
			String areaOpition, String areaSign, String atncOpition,
			String atncSign, String applyDate, String addressType,
			List<TeleGraphApplyEntity> apply,
			List<TeleGraphConfigEntity> config) {
		super();
		this.applyCom = applyCom;
		this.linkMan = linkMan;
		this.linkPhone = linkPhone;
		this.applyType = applyType;
		this.applyComSign = applyComSign;
		this.usingPeriod = usingPeriod;
		this.usingArea = usingArea;
		this.remark = remark;
		this.areaOpition = areaOpition;
		this.areaSign = areaSign;
		this.atncOpition = atncOpition;
		this.atncSign = atncSign;
		this.applyDate = applyDate;
		this.addressType = addressType;
		this.apply = apply;
		this.config = config;
	}
	
	public TeleGraphBaseInfoEntity(String applyCom, String serialNumber,String linkMan,
			String linkPhone, String applyType, String applyComSign,
			String usingPeriod, String usingArea, String remark,
			String areaOpition, String areaSign, String atncOpition,
			String atncSign, String applyDate, String addressType,
			List<TeleGraphApplyEntity> apply,
			List<TeleGraphConfigEntity> config,
			List<TeleGraphCircuitEntity> circuits) {
		super();
		this.applyCom = applyCom;
		this.serialNumber = serialNumber;
		this.linkMan = linkMan;
		this.linkPhone = linkPhone;
		this.applyType = applyType;
		this.applyComSign = applyComSign;
		this.usingPeriod = usingPeriod;
		this.usingArea = usingArea;
		this.remark = remark;
		this.areaOpition = areaOpition;
		this.areaSign = areaSign;
		this.atncOpition = atncOpition;
		this.atncSign = atncSign;
		this.applyDate = applyDate;
		this.addressType = addressType;
		this.apply = apply;
		this.config = config;
		this.circuits = circuits;
	}
